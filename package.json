{
  "name": "reports-app",
  "version": "1.0.0-beta.1",
  "private": true,
  "dependencies": {
    "@dhis2/d2-i18n": "^1.0.4",
    "@dhis2/d2-ui-app": "^2.0.0",
    "@dhis2/d2-ui-core": "^5.2.6",
    "@dhis2/d2-ui-forms": "^5.2.6",
    "@dhis2/d2-ui-header-bar": "^5.2.6",
    "@dhis2/d2-ui-org-unit-tree": "^5.2.6",
    "@dhis2/d2-ui-sharing-dialog": "^5.2.6",
    "@dhis2/d2-ui-table": "^5.2.6",
    "autoprefixer": "9.4.9",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.1.0",
    "babel-loader": "8.0.5",
    "babel-preset-react-app": "^7.0.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "chalk": "2.4.2",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.2.2",
    "css-loader": "2.1.0",
    "d2": "^31.4.0",
    "d2-manifest": "^1.0.0",
    "d3-color": "^1.2.0",
    "dotenv": "6.2.0",
    "dotenv-expand": "4.2.0",
    "eslint": "5.14.1",
    "eslint-config-react-app": "^3.0.7",
    "eslint-loader": "2.1.2",
    "eslint-plugin-flowtype": "3.4.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "3.0.1",
    "flexboxgrid": "^6.3.1",
    "fs-extra": "7.0.1",
    "history": "^4.7.2",
    "html-webpack-plugin": "3.2.0",
    "jest": "24.1.0",
    "lodash.debounce": "^4.0.8",
    "lodash.isempty": "^4.4.0",
    "material-ui": "^0.20.1",
    "moment": "^2.24.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "promise": "8.0.2",
    "prop-types": "^15.6.1",
    "raf": "3.4.1",
    "react": "^16.4.0",
    "react-dev-utils": "^7.0.3",
    "react-dom": "^16.4.0",
    "react-jss": "^8.4.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "resolve": "1.10.0",
    "style-loader": "0.23.1",
    "sw-precache-webpack-plugin": "0.11.5",
    "url-loader": "1.1.2",
    "webpack": "4.29.6",
    "webpack-dev-server": "3.2.1",
    "webpack-manifest-plugin": "2.0.4",
    "whatwg-fetch": "3.0.0",
    "xlsx": "^0.14.1"
  },
  "devDependencies": {
    "@dhis2/cli-style": "^2.0.1",
    "@dhis2/d2-i18n-generate": "^1.0.19",
    "chai": "^4.1.2",
    "cucumber": "^5.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "husky": "^1.3.1",
    "import-export": "^1.0.1",
    "jest-enzyme": "^7.0.1",
    "postcss-preset-env": "^6.5.0",
    "postcss-rtl": "^1.2.3",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.3",
    "wdio-allure-reporter": "^0.8.3",
    "wdio-cucumber-framework": "^2.2.8",
    "wdio-selenium-standalone-service": "0.0.12",
    "wdio-spec-reporter": "^0.1.4",
    "webdriverio": "^5.6.4"
  },
  "scripts": {
    "manifest": "d2-manifest package.json ./public/manifest.webapp",
    "start": "yarn manifest && yarn localize && node scripts/start.js",
    "build": "yarn manifest && yarn localize && node scripts/build.js && cp ./package.json ./build/package.json",
    "code-style": "yarn d2-style js",
    "format-all": "yarn prettier --write 'src/**/*.js'",
    "localize": "d2-i18n-generate -n reports-app -p ./i18n/ -o ./src/utils/i18n/locales/",
    "test": "node scripts/test.js --env=jsdom --coverage",
    "test-watch": "node scripts/test.js --env=jsdom",
    "test-e2e": "wdio ./e2e/config/wdio.conf.js",
    "lint": "yarn localize && eslint src/",
    "validate": "yarn list",
    "extract-pot": "node scripts/pot-extractor.js",
    "refresh-translations-template": "yarn extract-pot -- -o en.pot && git add ./i18n/en.pot",
    "coverage": "yarn test -- --coverage"
  },
  "husky": {
    "hooks": {
      "commit-msg": "yarn d2-style commit",
      "pre-commit": "yarn code-style"
    }
  },
  "manifest.webapp": {
    "name": "DHIS2 Reports App",
    "description": "DHIS2 reports App",
    "icons": {
      "48": "icon.png"
    },
    "developer": {
      "url": "https://dhis2.github.io",
      "name": "DHIS2"
    },
    "dhis2": {
      "apiVersion": "31"
    },
    "activities": {
      "dhis": {
        "href": ".."
      }
    }
  },
  "homepage": ".",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
